{
  "name": "Camplete AI-web",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve build/server/index.js",
    "typecheck": "tsc",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "docker:up": "docker compose --env-file .env up --build",
    "docker:down": "docker compose --env-file .env down --volumes --remove-orphans",
    "db:migrate": "docker compose exec app npx prisma migrate dev --schema=prisma-backend/prisma/schema.prisma",
    "db:generate": "docker compose exec app npx prisma generate --schema=prisma-backend/prisma/schema.prisma",
    "db:studio": "docker compose exec app npx prisma studio --schema=prisma-backend/prisma/schema.prisma"
  },
  "dependencies": {
    "@clerk/remix": "^4.10.4",
    "@prisma/client": "^6.12.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@remix-run/node": "^2.16.8",
    "@remix-run/react": "^2.16.8",
    "@remix-run/serve": "^2.16.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "google-ads-api": "^20.0.1",
    "googleapis": "^153.0.0",
    "isbot": "^4.1.0",
    "lucide-react": "^0.525.0",
    "match-sorter": "^6.3.1",
    "motion": "^12.23.6",
    "openai": "^5.10.2",
    "prisma": "^6.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^3.2.0",
    "sort-by": "^1.2.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "tiny-invariant": "^1.3.1",
    "uuid": "^11.1.0",
    "zod": "^3.25.76",
    "zod-form-data": "^3.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.16.8",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.47.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.1.6",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
